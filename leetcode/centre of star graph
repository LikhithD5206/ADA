class Solution {
    public int findCenter(int[][] edges) {
        
        // The center must appear in the first two edges
        if(edges[0][0] == edges[1][1] || edges[0][0] == edges[1][0]){
            return edges[0][0];
        }
        if(edges[0][1] == edges[1][1] || edges[0][1] == edges[1][0]){
            return edges[0][1];
        }
        return edges[0][0];
    }
}
